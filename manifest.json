{
  "manifest_version": 3,
  "name": "Geo UI Fixer",
  "version": "1.1",
  "description": "Toggle gutter fix and dark mode for Geo",
  "permissions": ["scripting", "storage", "tabs"],
  "host_permissions": [
    "https://geocreation.com.au/*",
    "https://demoapp.geocreation.com.au/*",
    "https://app.geocreation.com.au/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Geo UI Fixer"
  },
  "background": {
    "service_worker": "content.js"
  }
}
{
  "manifest_version": 3,
  "name": "chrome-custom-css",
  "version": "1.0",
  "description": "Overrides MUI toolbar padding on Geo apps to fix visual alignment.",
  "content_scripts": [
    {
      "matches": [
        "https://geocreation.com.au/*",
        "https://demoapp.geocreation.com.au/*",
        "https://app.geocreation.com.au/*"
      ],
      "js": ["content.js"],
      "css": ["fix.css"],
      "run_at": "document_idle"
    }
  ]
}